#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./globalinsurancesbapi clean install -DskipTests"
          bat "cd globalinsurancesbapi  & docker build -f dockerfile -t insappv1 ."
                                   
       }
   
   }
 
 }
 
 
 
}
